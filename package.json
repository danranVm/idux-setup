{
  "name": "idux-setup",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "pnpm -r --stream --filter=./packages/* run clean && rimraf dist node_modules pnpm-lock.yaml",
    "dev:admin": "pnpm -r --stream --filter=./packages/admin run dev",
    "dev:client": "pnpm -r --stream --filter=./packages/client run dev",
    "build": "run-p build:*",
    "build:admin": "pnpm -r --stream --filter=./packages/admin run build-only",
    "build:client": "pnpm -r --stream --filter=./packages/client run build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write packages/**/*.{ts,tsx,vue,less}",
    "lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@idux/cdk": "^1.8.1",
    "@idux/charts": "^0.4.0",
    "@idux/components": "^1.9.1",
    "@idux/pro": "^1.9.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/shared": "^10.1.2",
    "axios": "^1.3.5",
    "axios-retry": "^3.4.0",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.0",
    "lodash-es": "^4.17.0",
    "pinia": "^2.0.33",
    "pinia-plugin-persist": "^1.0.0",
    "query-string": "^8.1.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-angular": "^17.4.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.1",
    "less": "^4.1.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-static-copy": "^0.14.0",
    "vite-plugin-windicss": "^1.8.10",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0",
    "windicss": "^3.5.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "ts",
    "typescript",
    "vite",
    "admin",
    "admin-system",
    "admin-template",
    "idux",
    "idux-admin"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
